<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
    <schema path="/com/deepin/dde/audio/" id="com.deepin.dde.audio">
	<key type="b" name="first-run">
	    <default>true</default>
	    <summary>Whether has initialized</summary>
	    <description>If no initialized, do it, otherwise nothing to do.</description>
	</key>
	<key type="i" name="output-volume">
	    <default>50</default>
	    <summary>Output volume percentage</summary>
	    <description>Output volume percentage range 0 ~ 100</description>
	</key>
	<key type="i" name="headphone-output-volume">
	    <default>50</default>
	    <summary>Headphone output volume percentage</summary>
	    <description>Headphone output volume percentage range 0 ~ 100</description>
	</key>
	<key type="i" name="input-volume">
	    <default>50</default>
	    <summary>Input volume percentage</summary>
	    <description>Input volume percentage range 0 ~ 100</description>
	</key>
	<key type="i" name="output-volume-max">
	    <default>150</default>
	    <summary>Output volume max percentage</summary>
	    <description>The max percentage for output volume</description>
	</key>
	<key type="b" name="volume-increase">
	    <default>false</default>
	    <summary>switch sound increase</summary>
	    <description>if true, max volume be 1.5 ,else 1.0</description>
	</key>
	<key type="b" name="auto-switch-port">
	    <default>false</default>
	    <summary>Auto switch to available port</summary>
	    <description>If port available, switch to it.</description>
	</key>
	<key type="b" name="headphone-unplug-auto-pause">
	    <default>true</default>
	    <summary>Auto pause media when headphone unplug</summary>
	    <description>If headphone unplug and available, try pause all playing media.</description>
	</key>
	<key type="b" name="disable-auto-mute">
	    <default>false</default>
	    <summary>Whether to disable ALSA Mixer Auto-Mute Mode</summary>
	</key>
	<key type="b" name="reduce-input-noise">
	    <default>false</default>
	    <summary>Whether to reduce input noise</summary>
	</key>
	<key type="i" name="output-auto-switch-count-max">
			<default>50</default>
			<summary>Output auto switch count max</summary>
			<description>The output auto swich count max</description>
	</key>
    </schema>
</schemalist>
